{
  "version": 2,
  "builds": [
    {
      "src": "apps/backend/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/backend/src/main.ts"
    },
    {
      "src": "/health",
      "dest": "apps/backend/src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}